{
  "version": 2,
  "builds": [
    {
      "src": "sol_front.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "maxLambdaSize": "15mb",
        "includeFiles": ["solsocial.db"],
        "installCommand": "pip install -r requirements_vercel.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "sol_front.py",
      "methods": ["GET", "POST"]
    }
  ]
}
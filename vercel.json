{
  "version": 2,
  "builds": [
    {
      "src": "frontend/sol_front.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "maxLambdaSize": "50mb",
        "installCommand": "pip install -r frontend/requirements_vercel.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/sol_front.py"
    }
  ]
}
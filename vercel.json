{
  "version": 2,
  "builds": [
    {
      "src": "sol_front.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "sol_front.py",
      "methods": ["GET", "POST", "OPTIONS"]
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/sol_front.py" }
  ]
}
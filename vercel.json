{
  "version": 2,
  "builds": [
    {
      "src": "frontend/sol_front.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11",
        "maxLambdaSize": "15mb",
        "memory": 512,
        "installCommand": "cd frontend && pip install -r requirements_vercel.txt --prefix .. --no-warn-script-location"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "app.py"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/sol_front.py"
    }
  ]
}
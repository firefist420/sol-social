{
  "version": 2,
  "builds": [
    {
      "src": "frontend/sol_front.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11",
        "maxLambdaSize": "15mb",
        "memory": 512,
        "installCommand": "pip install --upgrade pip && pip install -r frontend/requirements_vercel.txt --no-deps --prefix ."
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/sol_front.py"
    }
  ]
}
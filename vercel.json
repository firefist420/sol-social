{
  "version": 2,
  "builds": [{
    "src": "app.py",
    "use": "@vercel/python",
    "config": {
      "runtime": "python3.11",
      "maxLambdaSize": "50mb",
      "installCommand": "pip install -r requirements.txt",
      "framework": "fastapi"
    }
  }],
  "routes": [{
    "src": "/(.*)",
    "dest": "app.py"
  }]
}
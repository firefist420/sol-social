{
  "version": 2,
  "builds": [
    {
      "src": "sol_front.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://api.solsocial.social/api/$1",
      "headers": {
        "Access-Control-Allow-Origin": "https://solsocial.social",
        "Access-Control-Allow-Methods": "GET,POST,PUT,DELETE,OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type,Authorization"
      }
    },
    {
      "src": "/(.*)",
      "dest": "sol_front.py"
    }
  ]
}